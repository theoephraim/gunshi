{
  "$id": "https://jsr.io/schema/config-file.v1.json",
  "$schema": "http://json-schema.org/draft-07/schema",
  "name": "@kazupon/gunshi",
  "description": "Modern javascript command-line library",
  "version": "0.3.1",
  "license": "MIT",
  "publish": {
    "include": [
      "src/**/*.ts",
      "locales/*.json",
      "package.json",
      "jsr.json",
      "README.md",
      "CHANGELOG.md",
      "LICENSE"
    ],
    "exclude": ["src/**/*.test.ts", "src/**/*.test-d.ts", "**/__snapshots__/**"]
  },
  "exports": {
    ".": "./src/index.ts",
    "./context": "./src/context.ts",
    "./renderer": "./src/renderer/index.ts"
  }
}
