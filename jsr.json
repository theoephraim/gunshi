{
  "$id": "https://jsr.io/schema/config-file.v1.json",
  "$schema": "http://json-schema.org/draft-07/schema",
  "name": "@kazupon/gunshi",
  "description": "Modern javascript command-line library",
  "version": "0.14.4",
  "license": "MIT",
  "publish": {
    "include": [
      "src/**/*.ts",
      "src/locales/*.json",
      "assets/*.png",
      "package.json",
      "jsr.json",
      "README.md",
      "CHANGELOG.md",
      "LICENSE"
    ],
    "exclude": [
      "src/**/*.test.ts",
      "src/**/*.test-d.ts",
      "docs/.vitepress/config.ts",
      "**/__snapshots__/**",
      "**/dist/**"
    ]
  },
  "exports": {
    ".": "./src/index.ts",
    "./renderer": "./src/renderer.ts",
    "./definition": "./src/definition.ts",
    "./generator": "./src/generator.ts"
  }
}
