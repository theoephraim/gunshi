{
  "$id": "https://jsr.io/schema/config-file.v1.json",
  "$schema": "http://json-schema.org/draft-07/schema",
  "name": "@gunshi/resources",
  "description": "built-in localization resources for gunshi",
  "version": "0.26.3",
  "license": "MIT",
  "publish": {
    "include": [
      "src/**/*.ts",
      "locales/**/*.json",
      "package.json",
      "jsr.json",
      "README.md",
      "CHANGELOG.md",
      "LICENSE"
    ],
    "exclude": [
      "src/**/*.test.ts",
      "src/**/*.test-d.ts",
      "**/__snapshots__/**",
      "**/lib/**",
      "**/dist/**"
    ]
  },
  "exports": {
    ".": "./src/index.ts",
    "./en-US": "./locales/en-US.json",
    "./ja-JP": "./locales/ja-JP.json"
  }
}
