{
  "name": "gunshi",
  "description": "Modern javascript command-line library",
  "version": "0.12.0",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/kazupon",
  "bugs": {
    "url": "https://github.com/kazupon/gunshi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazupon/gunshi.git"
  },
  "keywords": [
    "command",
    "option",
    "cli",
    "argument",
    "args",
    "argv"
  ],
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.7.0",
  "engines": {
    "node": ">= 20"
  },
  "type": "module",
  "files": [
    "lib"
  ],
  "module": "lib/index.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./renderer": {
      "types": "./lib/renderer/index.d.ts",
      "import": "./lib/renderer/index.js",
      "require": "./lib/renderer/index.js",
      "default": "./lib/renderer/index.js"
    },
    "./generator": {
      "types": "./lib/generator.d.ts",
      "import": "./lib/generator.js",
      "require": "./lib/generator.js",
      "default": "./lib/generator.js"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "build": "tsdown",
    "changelog": "gh-changelogen --repo=kazupon/gunshi",
    "clean": "git clean -df",
    "dev": "pnpx @eslint/config-inspector --config eslint.config.ts",
    "dev:eslint": "pnpx @eslint/config-inspector --config eslint.config.ts",
    "dev:typedoc": "typedoc --watch --preserveWatchOutput",
    "docs:build": "pnpm run docs:build:typedoc && pnpm docs:build:vitepress",
    "docs:build:typedoc": "typedoc",
    "docs:build:vitepress": "vitepress build docs",
    "docs:dev": "pnpm run docs:build:typedoc && pnpm docs:dev:vitepress",
    "docs:dev:vitepress": "vitepress dev docs",
    "docs:preview": "vitepress preview docs",
    "fix": "pnpm run --stream --color \"/^fix:/\"",
    "fix:eslint": "eslint . --fix",
    "fix:knip": "knip --fix --no-exit-code",
    "fix:prettier": "prettier . --write",
    "lint": "pnpm run --stream --color \"/^lint:/\"",
    "lint:eslint": "eslint .",
    "lint:jsr": "jsr publish --dry-run --allow-dirty",
    "lint:knip": "knip",
    "lint:prettier": "prettier . --check",
    "prepack": "pnpm build",
    "prepare": "git config --local core.hooksPath .githooks",
    "release": "bumpp --commit \"release: v%s\" --all --push --tag",
    "test": "vitest run",
    "typecheck": "pnpm run --stream --color \"/^typecheck:/\"",
    "typecheck:deno": "deno check --all ./src",
    "typecheck:tsc": "tsc --noEmit"
  },
  "dependencies": {
    "args-tokens": "^0.14.0"
  },
  "devDependencies": {
    "@eslint/markdown": "^6.3.0",
    "@intlify/core": "next",
    "@kazupon/eslint-config": "^0.26.1",
    "@kazupon/prettier-config": "^0.1.1",
    "@types/node": "^22.13.14",
    "@vitest/eslint-plugin": "^1.1.38",
    "bumpp": "^10.1.0",
    "deno": "^2.2.6",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.2.7",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsonc": "^2.20.0",
    "eslint-plugin-module-interop": "^0.3.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-yml": "^1.17.0",
    "gh-changelogen": "^0.2.8",
    "jsr": "^0.13.4",
    "knip": "^5.46.3",
    "lint-staged": "^15.5.0",
    "messageformat": "4.0.0-10",
    "pkg-pr-new": "^0.0.42",
    "prettier": "^3.5.3",
    "tsdown": "^0.6.10",
    "typedoc": "^0.28.1",
    "typedoc-plugin-markdown": "^4.6.0",
    "typedoc-vitepress-theme": "^1.1.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vitepress": "^1.6.3",
    "vitepress-plugin-group-icons": "^1.3.8",
    "vitepress-plugin-llms": "^0.0.21",
    "vitest": "^3.0.9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "prettier": "@kazupon/prettier-config",
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ],
    "*.{js,mjs,cjs}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,jsonc,json5,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
